// Это комментарий

// Объявление модуля

МОДУЛЬ ТЕСТ

// Содержимое модуля

ПУСТЬ Ф1() = 2                    // функция константы

ПУСТЬ Ф2(ИКС) = ИКС               // функции от аргумента
ПУСТЬ УДВ(ИКС) = СЛОЖ(ИКС, ИКС)

ПУСТЬ Ф3() = УДВ                  // функции - объекты первого порядка
ПУСТЬ Ф4(ИКС, Ф) = Ф(ИКС)

ПУСТЬ Ф5() = СПИСОК(1, Ф2(Ф1()))  // функции список

ПУСТЬ Ф6(ИКС) = СПИСОК(
  ИКС,
  Ф4(Ф1(), УДВ),
  5,
  Ф3()(ИКС)
)

ПУСТЬ Ф7() = Ф6(3)               // тоже равна списку

ПУСТЬ Ф8() = СПИСОК(Ф5(), Ф7())  // списки можно объединить
ПУСТЬ Ф9() = СПИСОК(Ф8(), 7)

ПУСТЬ ФС1() = "Привет Мир! "     // строковые константы являются списками
ПУСТЬ ФС2() = "Привет "
ПУСТЬ ФС3(ИМЯ) = ИМЯ

ПУСТЬ ФС4(ИМЯ) = СПИСОК(         // объединяет в одну строку
  ФС1(),
  ФС2(),
  ФС3(ИМЯ)
)

// Объявление функции с побочными эффектами

ПУСТЬ Ф(ИМЯ) =
  ВЫВОДС(ФС4(ИМЯ)),              // эффект вывод списка как строки
  ВЫВОД(Ф9()),                   // эффект вывод значений
  0                              // возврат результата

// определяет главную функцию модуля

СТАРТ Ф
