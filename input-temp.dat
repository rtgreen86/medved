let F1() = "Hello World!"
let F2(name) = "Hello " | name
let SUM(A, B) = A + B;
let M1(A) = A - 1;

// define another function with side effect
let F(name) =
  outstr(F1()),
  outstr(F2(name)),
  M1(SUM(2, 2))

